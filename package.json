{
  "name": "htl-vscode-settings",
  "displayName": "HTL vscode settings",
  "publisher": "HossainiHosain",
  "description": "This is a simple VSCode extension that sets some default settings for the editor and prettier.",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/hossaini310/htl-vscode-settings"
  },
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "deactivationEvents": [
    "onDeactivate"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "htl-vscode-settings.enable",
        "title": "Enable HTL vscode settings"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
